meta {
  name: Change Password
  type: http
  seq: 9
}

post {
  url: http://{{ilim-url}}/auth/change-password
  body: json
  auth: bearer
}

auth:bearer {
  token: {{token}}
}

body:json {
  {
    "oldPassword": "{{eduPassword}}",
    "newPassword": "{{eduPassword}}"
  }
}

assert {
  status: eq 200
}
