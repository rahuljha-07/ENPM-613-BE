meta {
  name: Inst can add video to module
  type: http
  seq: 11
}

post {
  url: http://{{ilim-url}}/instructor/module/{{moduleId}}/add-video
  body: json
  auth: bearer
}

auth:bearer {
  token: {{instructorToken}}
}

body:json {
  {
    "title": "Video Tarzan (test run)",
    "description": "some text heeereee",
    "videoUrl": "https://domain.org/video.mpeg",
    "durationInSeconds": 80
  }
}

assert {
  res.status: eq 201
  res.body.body: isDefined
}
