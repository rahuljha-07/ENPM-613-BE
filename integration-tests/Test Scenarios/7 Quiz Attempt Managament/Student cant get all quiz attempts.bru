meta {
  name: Student cant get all quiz attempts
  type: http
  seq: 24
}

get {
  url: http://{{ilim-url}}/instructor/quiz-attempt/{{quizId}}
  body: json
  auth: bearer
}

auth:bearer {
  token: {{studentToken}}
}

body:json {
  {
    "quizId": "{{quizId}}",
    "answers": [
      {
        "questionId": "{{questions1Id}}",
        "selectedOptionIds": [
          "{{questions1Option2}}"
        ]
      },
      {
        "questionId": "{{questions2Id}}",
        "selectedOptionIds": [
          "{{questions2Option1}}",
          "{{questions2Option3}}"
        ]
      }
    ]
  }
}

assert {
  res.status: eq 403
}
