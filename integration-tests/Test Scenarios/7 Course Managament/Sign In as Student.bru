meta {
  name: Sign In as Student
  type: http
  seq: 1
}

post {
  url: http://{{ilim-url}}/auth/sign-in
  body: json
  auth: none
}

body:json {
  {
      "email": "user@mail.com",
      "password": "{{eduPassword}}"
  }
}

vars:post-response {
  studentToken: res.body.accessToken
}

assert {
  res.status: eq 200
}
