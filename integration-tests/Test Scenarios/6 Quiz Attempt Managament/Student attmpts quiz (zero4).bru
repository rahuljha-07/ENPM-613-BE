meta {
  name: Student attmpts quiz (zero4)
  type: http
  seq: 22
}

post {
  url: http://{{ilim-url}}/student/attempt-quiz
  body: json
  auth: bearer
}

auth:bearer {
  token: {{studentToken}}
}

body:json {
  {
    "quizId": "{{quizId}}",
    "answers": [
      {
        "questionId": "{{questions1Id}}",
        "selectedOptionIds": [
          "{{questions1Option2}}"
        ]
      },
      {
        "questionId": "{{questions2Id}}",
        "selectedOptionIds": [
          "{{questions2Option1}}",
          "{{questions2Option3}}"
        ]
      }
    ]
  }
}

vars:post-response {
  attemptId: res.body.body.attemptId
}

assert {
  res.status: eq 200
  res.body.body.userScore: eq 0
  res.body.body.totalScore: eq 10
  res.body.body.passed: eq false
}
