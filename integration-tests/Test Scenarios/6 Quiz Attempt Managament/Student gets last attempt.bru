meta {
  name: Student gets last attempt
  type: http
  seq: 24
}

get {
  url: http://{{ilim-url}}/student/quiz-attempt/{{quizId}}/last
  body: json
  auth: bearer
}

auth:bearer {
  token: {{studentToken}}
}

body:json {
  {
    "quizId": "{{quizId}}",
    "answers": [
      {
        "questionId": "{{questions1Id}}",
        "selectedOptionIds": [
          "{{questions1Option2}}"
        ]
      },
      {
        "questionId": "{{questions2Id}}",
        "selectedOptionIds": [
          "{{questions2Option1}}",
          "{{questions2Option3}}"
        ]
      }
    ]
  }
}

assert {
  res.status: eq 200
  res.body.body.userScore: eq 0
  res.body.body.totalScore: eq 10
  res.body.body.passed: eq false
  res.body.body.attemptId: eq {{attemptId}}
}
