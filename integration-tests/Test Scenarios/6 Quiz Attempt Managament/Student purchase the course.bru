meta {
  name: Student purchase the course
  type: http
  seq: 11
}

post {
  url: http://{{ilim-url}}/student/purchase-course/{{courseId}}
  body: json
  auth: bearer
}

auth:bearer {
  token: {{studentToken}}
}

body:json {
  {
      "title": "CS Course 4",
      "description": "A real course for beginners in CS",
      "price": 100.00,
      "thumbnailUrl": "https://domain.org/image2.png"
  }
}

assert {
  res.status: eq 200
}
