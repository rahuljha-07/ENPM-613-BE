meta {
  name: Instructor can access created module
  type: http
  seq: 7
}

get {
  url: http://{{ilim-url}}/module/{{moduleId}}
  body: none
  auth: bearer
}

auth:bearer {
  token: {{instructorToken}}
}

body:json {
  {
      "title": "Computer Science Course",
      "description": "This is a new course for beginners to teach them computer science",
      "price": 54.15,
      "thumbnailUrl": "https://domain.org/image.png"
  }
}

vars:post-response {
  courseId: res.body.body.courseId
}

assert {
  res.status: eq 200
  res.body.body.id: isDefined
}
